{
  "name": "fantamd-be",
  "version": "1.0.0",
  "main": "app.js",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "npm run build && fastify start -l info dist/app.js",
    "build": "tsc",
    "test": "c8 npm run test:run",
    "test:run": "glob -c \"tsx --test\" \"./test/**/*.ts\"",
    "standalone": "npm run build && node --env-file=.env dist/server.js",
    "lint": "eslint --ignore-pattern=dist",
    "lint:fix": "npm run lint -- --fix",
    "build:functions": "netlify-lambda build functions --config ./webpack.config.netlify.js"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@fastify/auth": "^5.0.2",
    "@fastify/autoload": "^6.3.1",
    "@fastify/aws-lambda": "^6.1.1",
    "@fastify/cors": "^11.0.1",
    "@fastify/env": "^5.0.2",
    "@fastify/jwt": "^9.1.0",
    "@fastify/postgres": "^6.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/sensible": "^6.0.3",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@fastify/type-provider-typebox": "^5.2.0",
    "@sinclair/typebox": "^0.34.37",
    "fastify": "^5.4.0",
    "fastify-cli": "^7.4.0",
    "fastify-plugin": "^5.0.1",
    "i": "^0.3.7",
    "knex": "^3.1.0",
    "npm": "^11.4.2",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/node": "^24.0.14",
    "c8": "^10.1.3",
    "eslint": "^9.31.0",
    "fastify-tsconfig": "^3.0.0",
    "form-data": "^4.0.3",
    "neostandard": "^0.12.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
